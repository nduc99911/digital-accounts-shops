# Copy to .env
# Postgres (dev)
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/digital_accounts_shops?schema=public"

# Admin default (created on first /admin/login)
ADMIN_USERNAME="admin"
ADMIN_PASSWORD="admin12345"

# Optional
SHOP_NAME="taikhoanso.com"
NEXT_PUBLIC_SHOP_NAME="taikhoanso.com"

# NextAuth Configuration
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-random-secret-key-here"

# Google OAuth (get from Google Cloud Console)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""

# Facebook OAuth (get from Facebook Developers)
FACEBOOK_CLIENT_ID=""
FACEBOOK_CLIENT_SECRET=""

# Email (Resend - get API key from https://resend.com)
RESEND_API_KEY="re_xxxxxxxx"
FROM_EMAIL="noreply@taikhoanso.com"
